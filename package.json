    "echarts-liquidfill": "^3.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "rxjs": "^7.8.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@kalimahapps/vue-icons": "^1.4.1",
    "@tanzerfe/eslint-config-lint": "^0.0.7",
    "@tanzerfe/http": "^1.0.0",
